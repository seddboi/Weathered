{
	"name": "server",
	"version": "1.0.0",
	"description": "react express server",
	"main": "index.js",
	"scripts": {
		"client-install": "cd client && npm install",
		"start": "node index.js --openssl-legacy-provider start",
		"server": "nodemon index.js --openssl-legacy-provider start",
		"build": "cd client && react-scripts build",
		"client": "cd client && npm start",
		"dev": "concurrently \"npm run server\" \"npm run client\""
	},
	"author": "Gian Z",
	"license": "MIT",
	"dependencies": {
		"axios": "^0.26.0",
		"concurrently": "^7.0.0",
		"cors": "^2.8.5",
		"dotenv": "^16.0.0",
		"express": "^4.17.3",
		"nodemon": "^2.0.15"
	}
}
